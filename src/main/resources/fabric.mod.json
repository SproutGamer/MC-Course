{
	"schemaVersion": 1,
	"id": "mccourse",
	"version": "${version}",
	"name": "MC Course",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"SproutGamer"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0",
	"icon": "assets/mccourse/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "sproutgamer.mods.mccourse.MCCourse",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "sproutgamer.mods.mccourse.MCCourseClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "sproutgamer.mods.mccourse.MCCourseDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"mccourse.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "0.110.0+1.21.1",
		"fabric-language-kotlin": "1.13.0+kotlin.2.1.0"
	},
	"suggests": {
		"another-mod": "*"
	}
}